<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="itc.hoseo.soolfarm.admin.banner.impl.BannerDao">

	<select id="getBanners" parameterType="Banner" resultType="Banner">
	SELECT * FROM BANNER
		<where>
			<if test="startDttm != null">
				start_dttm <![CDATA[ >= ]]> #{startDttm}
			</if>
			<if test="endDttm != null">
				end_dttm <![CDATA[ >=  ]]> #{endDttm} OR end_dttm is null;
			</if>
		</where>
	</select>

</mapper>